{
  "dependencies": {
    "@clerk/nextjs": "catalog:",
    "@supabase/ssr": "catalog:",
    "@supabase/supabase-js": "catalog:",
    "@t3-oss/env-core": "catalog:",
    "@unhook/id": "workspace:*",
    "@unhook/logger": "workspace:*",
    "@unhook/zustand": "workspace:*",
    "@vercel/postgres": "catalog:",
    "date-fns": "catalog:",
    "drizzle-orm": "catalog:",
    "drizzle-seed": "catalog:",
    "drizzle-zod": "catalog:",
    "lodash-es": "catalog:",
    "next": "catalog:",
    "pg": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@t3-oss/env-core": "catalog:",
    "@t3-oss/env-nextjs": "catalog:",
    "@types/lodash-es": "catalog:",
    "@types/pg": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@unhook/tsconfig": "workspace:*",
    "chokidar": "catalog:",
    "drizzle-kit": "catalog:",
    "supabase": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "zx": "catalog:"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./schema": "./src/schema.ts",
    "./supabase/cli": "./src/supabase/cli.ts",
    "./supabase/client": "./src/supabase/client-exports.ts",
    "./supabase/create-channel": "./src/supabase/create-channel.ts",
    "./supabase/rcs": "./src/supabase/server.rcs.ts",
    "./supabase/server": "./src/supabase/server-exports.ts",
    "./supabase/types": "./src/supabase/types.generated.ts"
  },
  "license": "MIT",
  "name": "@unhook/db",
  "private": true,
  "scripts": {
    "build.skip": "bun run gen-supabase-types",
    "clean": "git clean -xdf .turbo node_modules dist",
    "create-supabase-policies": "tsx scripts/create-supabase-policies.ts",
    "dev": "tsx ./scripts/watch.ts",
    "enable-supabase-realtime": "tsx scripts/enable-supabase-realtime.ts",
    "gen-migration": "drizzle-kit generate",
    "gen-supabase-types": "tsx ./scripts/generate-supabase-types.ts",
    "migrate": "drizzle-kit migrate && bun gen-supabase-types",
    "push": "drizzle-kit push && bun gen-supabase-types && bun create-supabase-policies && bun enable-supabase-realtime",
    "seed": "tsx src/seed.ts",
    "studio": "drizzle-kit studio",
    "supabase:local": "supabase start",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "type": "module",
  "version": "0.1.0"
}
