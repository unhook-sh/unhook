{
  "dependencies": {
    "@clerk/nextjs": "6.35.2",
    "@supabase/ssr": "0.7.0",
    "@supabase/supabase-js": "2.83.0",
    "@t3-oss/env-core": "0.13.8",
    "@unhook/id": "workspace:*",
    "@unhook/logger": "workspace:*",
    "@unhook/stripe": "workspace:*",
    "@unhook/zustand": "workspace:*",
    "@vercel/postgres": "0.10.0",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.44.7",
    "drizzle-seed": "0.3.1",
    "drizzle-zod": "0.8.3",
    "lodash-es": "4.17.21",
    "next": "16.0.3",
    "pg": "8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "4.1.12",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@t3-oss/env-core": "0.13.8",
    "@t3-oss/env-nextjs": "0.13.8",
    "@types/lodash-es": "4.17.12",
    "@types/pg": "8.15.6",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "@unhook/tsconfig": "workspace:*",
    "chokidar": "4.0.3",
    "drizzle-kit": "0.31.7",
    "supabase": "2.58.5",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "zx": "8.8.5"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./schema": "./src/schema.ts",
    "./supabase/cli": "./src/supabase/cli.ts",
    "./supabase/client": "./src/supabase/client-exports.ts",
    "./supabase/create-channel": "./src/supabase/create-channel.ts",
    "./supabase/get-realtime-record": "./src/supabase/get-realtime-record.ts",
    "./supabase/rcs": "./src/supabase/server.rcs.ts",
    "./supabase/server": "./src/supabase/server-exports.ts",
    "./supabase/types": "./src/supabase/types.generated.ts"
  },
  "license": "MIT",
  "name": "@unhook/db",
  "private": true,
  "scripts": {
    "build.skip": "bun run gen-supabase-types",
    "clean": "git clean -xdf .turbo node_modules dist",
    "create-supabase-policies": "tsx scripts/create-supabase-policies.ts",
    "dev": "tsx ./scripts/watch.ts",
    "enable-supabase-realtime": "tsx scripts/enable-supabase-realtime.ts",
    "gen-migration": "drizzle-kit generate",
    "gen-supabase-types": "tsx ./scripts/generate-supabase-types.ts",
    "migrate": "drizzle-kit migrate && bun gen-supabase-types && bun create-supabase-policies && bun enable-supabase-realtime",
    "push": "drizzle-kit push && bun gen-supabase-types && bun create-supabase-policies && bun enable-supabase-realtime",
    "seed": "tsx src/seed.ts",
    "studio": "drizzle-kit studio",
    "supabase:local": "supabase start",
    "test:integ": "bun test --test=integ",
    "test:unit": "bun test --test=unit",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "type": "module",
  "version": "0.1.0"
}
