{
  "author": "Unhook Team (https://github.com/unhook-sh)",
  "bin": {
    "unhook": "./bin/unhook"
  },
  "bugs": {
    "url": "https://github.com/unhook-sh/unhook/issues"
  },
  "cpu": ["x64", "arm64"],
  "dependencies": {
    "@clerk/backend": "1.28.0",
    "@clerk/clerk-js": "5.67.0",
    "@clerk/express": "1.5.0",
    "@clerk/types": "4.59.0",
    "@t3-oss/env-core": "0.13.4",
    "@unhook/api": "workspace:*",
    "@unhook/client": "workspace:*",
    "@unhook/db": "workspace:*",
    "@unhook/id": "workspace:*",
    "@unhook/logger": "workspace:*",
    "@unhook/npm": "workspace:*",
    "@unhook/zustand": "workspace:*",
    "chalk": "5.4.1",
    "cli-highlight": "2.1.11",
    "cli-spinners": "3.2.0",
    "clipboardy": "4.0.0",
    "date-fns": "4.1.0",
    "dedent-js": "1.0.1",
    "drizzle-orm": "0.43.1",
    "figlet": "1.8.1",
    "figures": "6.1.0",
    "ink": "5.2.1",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.2",
    "keytar": "7.9.0",
    "meow": "13.2.0",
    "open": "10.1.2",
    "posthog-js": "1.242.2",
    "react": "19.1.0",
    "react-devtools": "6.1.2",
    "react-devtools-core": "6.1.2",
    "react-dom": "19.1.0",
    "react-error-boundary": "6.0.0",
    "react-reconciler": "0.32.0",
    "type-fest": "4.41.0",
    "undici": "7.9.0",
    "yargs": "17.7.2",
    "zod": "3.24.4",
    "zustand": "5.0.4",
    "zx": "8.5.4"
  },
  "description": "A CLI tool for testing webhooks locally with team collaboration features.",
  "devDependencies": {
    "@happy-dom/global-registrator": "17.4.7",
    "@playwright/test": "^1.52.0",
    "@types/bun": "1.2.14",
    "@types/figlet": "1.7.0",
    "@types/js-yaml": "4.0.9",
    "@types/jsonwebtoken": "9.0.9",
    "@types/react": "19.1.4",
    "@types/react-devtools": "3.6.2",
    "@types/react-dom": "19.1.5",
    "@types/ws": "^8.18.1",
    "@types/yargs": "17.0.33",
    "@unhook/tsconfig": "workspace:*",
    "ink-testing-library": "4.0.0",
    "is-ci": "4.1.0",
    "typescript": "5.8.3",
    "ws": "^8.18.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "default": "./src/exports.ts",
      "types": "./dist/src/exports.d.ts"
    }
  },
  "files": [
    "bin",
    "README.md",
    "LICENSE",
    "dist/src/exports.d.ts",
    "scripts/install.cjs"
  ],
  "homepage": "https://github.com/unhook-sh/unhook#readme",
  "keywords": [
    "webhook",
    "development",
    "testing",
    "cli",
    "webhook",
    "local-development",
    "webhook-testing",
    "webhook-development",
    "team-collaboration",
    "real-time",
    "monitoring",
    "debugging",
    "stripe-webhooks",
    "github-webhooks",
    "slack-webhooks",
    "clerk-webhooks"
  ],
  "license": "MIT",
  "name": "@unhook/cli",
  "os": ["darwin", "linux"],
  "pnpm": {
    "overrides": {
      "@clerk/shared": {
        "trusted": true
      },
      "browser-tabs-lock": {
        "trusted": true
      },
      "core-js": {
        "trusted": true
      },
      "electron": {
        "trusted": true
      },
      "keytar": {
        "trusted": true
      }
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "apps/cli",
    "type": "git",
    "url": "git+https://github.com/unhook-sh/unhook.git"
  },
  "scripts": {
    "build": "bun run build:base --minify --production",
    "build:base": "NEXT_PUBLIC_APP_TYPE=cli bun build ./src/cli.tsx --outfile ./bin/unhook --env 'NEXT_PUBLIC_*' --compile --target ${BUN_BUILD_TARGET:-bun} --bundle --external none",
    "clean": "git clean -xdf bin .turbo node_modules dist",
    "dev": "bun run build:base --watch --sourcemap=inline",
    "postinstall.skip": "node -e \"require('is-ci') ? process.exit(0) : require('./scripts/install.cjs')\"",
    "prepublishOnly": "bun typecheck && bun run build && prepare-publish",
    "start": "bun run ./src/cli.tsx",
    "start:debug": "DEV=true bun run ./src/cli.tsx",
    "test": "bun test",
    "typecheck": "tsc"
  },
  "trustedDependencies": [
    "@clerk/shared",
    "browser-tabs-lock",
    "core-js",
    "electron",
    "keytar"
  ],
  "type": "module",
  "version": "0.9.2"
}
