{
  "bin": {
    "unhook": "./bin/unhook"
  },
  "dependencies": {
    "@clerk/clerk-js": "5.59.1",
    "@clerk/types": "4.51.0",
    "@t3-oss/env-core": "0.12.0",
    "@unhook/api": "workspace:*",
    "@unhook/db": "workspace:*",
    "@unhook/id": "workspace:*",
    "@unhook/tunnel": "workspace:*",
    "chalk": "5.4.1",
    "cli-highlight": "2.1.11",
    "cli-spinners": "3.2.0",
    "date-fns": "4.1.0",
    "debug": "4.4.0",
    "dedent-js": "1.0.1",
    "drizzle-orm": "0.41.0",
    "figlet": "1.8.0",
    "figures": "6.1.0",
    "ink": "5.2.0",
    "ink-select-input": "6.0.0",
    "ink-spinner": "5.0.0",
    "ink-table": "3.1.0",
    "ink-text-input": "6.0.0",
    "meow": "13.2.0",
    "open": "10.1.0",
    "picocolors": "1.1.1",
    "react": "19.1.0",
    "react-devtools-core": "6.1.1",
    "react-dom": "19.1.0",
    "react-error-boundary": "5.0.0",
    "react-reconciler": "0.32.0",
    "type-fest": "4.39.1",
    "yargs": "17.7.2",
    "zod": "3.24.2",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/debug": "4.1.12",
    "@types/figlet": "1.7.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@types/yargs": "17.0.33",
    "@unhook/tsconfig": "workspace:*",
    "typescript": "5.8.3"
  },
  "license": "MIT",
  "name": "@unhook/cli",
  "private": false,
  "files": ["bin", "README.md", "LICENSE"],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "echo \"BUN_BUILD_TARGET=$BUN_BUILD_TARGET\" && bun build ./src/cli.ink.tsx ./src/templates/auth-success.html --outfile ./bin/unhook --minify --sourcemap --env 'NEXT_PUBLIC_*' --compile --target ${BUN_BUILD_TARGET:-bun} --asset-naming \"[name].[ext]\"",
    "dev": "pnpm with-env pnpm build --watch",
    "copy-readme": "cp ../../README.md ./README.md",
    "prepublishOnly": "pnpm copy-readme && pnpm build",
    "start": "pnpm with-env bun run ./src/cli.ink.tsx",
    "start:debug": "DEV=true pnpm with-env bun run ./src/cli.ink.tsx",
    "test": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "infisical run -- "
  },
  "type": "module",
  "version": "0.3.0",
  "keywords": [
    "webhook",
    "development",
    "testing",
    "cli",
    "tunnel",
    "local-development",
    "webhook-testing",
    "webhook-development",
    "team-collaboration",
    "real-time",
    "monitoring",
    "debugging",
    "stripe-webhooks",
    "github-webhooks",
    "slack-webhooks",
    "clerk-webhooks"
  ]
}
