{
  "autoClosingPairs": [["{", "}"], ["[", "]"], ["\"", "\""]],
  "brackets": [["{", "}"], ["[", "]"]],
  "comments": {
    "blockComment": ["/*", "*/"],
    "lineComment": "//"
  },
  "folding": {
    "markers": {
      "end": "^\\s*//\\s*endregion\\b",
      "start": "^\\s*//\\s*region\\b"
    }
  },
  "indentationRules": {
    "decreaseIndentPattern": "^\\s*[}\\]],?\\s*$",
    "increaseIndentPattern": "^\\s*[{[]\\s*$"
  },
  "language": "json",
  "surroundingPairs": [["{", "}"], ["[", "]"], ["\"", "\""]],
  "wordPattern": "(-?\\b\\d+\\.\\d+\\b)|(-?\\b\\d+\\b)|([a-zA-Z_][a-zA-Z0-9_]*)|([\"'][^\"']*[\"'])"
}
