{
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.3",
    "@unhook/logger": "workspace:*",
    "mcp-handler": "1.0.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@unhook/tsconfig": "workspace:*",
    "typescript": "5.9.2"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "main": "./src/index.ts",
  "name": "@unhook/mcp-server",
  "private": true,
  "scripts": {
    "build.skip": "tsup",
    "clean": "git clean -xdf .turbo node_modules dist",
    "debug:server": "bun run src/debug-server.ts",
    "dev": "bunx @modelcontextprotocol/inspector bun run src/debug-server.ts",
    "dev:inspector": "bun run src/dev-inspector.ts",
    "dev:server": "bun run src/dev-server.ts",
    "example": "tsx src/example.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "types": "./src/index.ts",
  "version": "0.0.0"
}
