  "description": "A CLI tool for testing webhooks locally with team collaboration features.",
  "devDependencies": {
    "@types/figlet": "1.7.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "19.1.1",
    "@types/react-devtools": "3.6.2",
    "@types/react-dom": "19.1.2",
    "@types/yargs": "17.0.33",
    "@unhook/tsconfig": "workspace:*",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": ["bin", "README.md", "LICENSE"],
  "homepage": "https://github.com/unhook-sh/unhook#readme",
  "keywords": [
    "webhook",
    "development",
    "testing",
    "cli",
    "tunnel",
    "local-development",
    "webhook-testing",
    "webhook-development",
    "team-collaboration",
    "real-time",
    "monitoring",
    "debugging",
    "stripe-webhooks",
    "github-webhooks",
    "slack-webhooks",
    "clerk-webhooks"
  ],
  "license": "MIT",
  "name": "@unhook/cli",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unhook-sh/unhook.git"
  },
  "scripts": {
    "build": "NEXT_PUBLIC_APP_TYPE=cli bun build ./src/cli.tsx --outfile ./bin/unhook --minify --sourcemap --env 'NEXT_PUBLIC_*' --compile --target ${BUN_BUILD_TARGET:-bun}",
    "clean": "git clean -xdf bin .turbo node_modules dist",
    "copy-readme": "cp ../../README.md ./README.md",
    "dev": "bun with-env bun run build --watch",
    "prepublishOnly": "bun typecheck && bun copy-readme && bun build",
    "start": "bun with-env bun run ./src/cli.tsx",
    "start:debug": "DEV=true bun with-env bun run ./src/cli.tsx",
    "test": "bun with-env bun test",
    "typecheck": "tsc",
    "with-env": "infisical run -- "
  },
  "type": "module",
  "version": "0.3.0"
}
