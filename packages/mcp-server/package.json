{
  "dependencies": {
    "@clerk/nextjs": "6.25.0",
    "@modelcontextprotocol/sdk": "1.15.1",
    "@unhook/analytics": "workspace:*",
    "@unhook/api": "workspace:*",
    "@unhook/db": "workspace:*",
    "@unhook/logger": "workspace:*",
    "@vercel/mcp-adapter": "1.0.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/node": "24.0.14",
    "@unhook/tsconfig": "workspace:*",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "main": "./src/index.ts",
  "name": "@unhook/mcp-server",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "git clean -xdf .turbo node_modules dist",
    "debug:server": "bun run src/debug-server.ts",
    "dev:inspector": "bun run src/dev-inspector.ts",
    "dev:server": "bun run src/dev-server.ts",
    "example": "tsx src/example.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "types": "./src/index.ts",
  "version": "0.0.0"
}
