{
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "brackets": [["{", "}"], ["[", "]"], ["(", ")"]],
  "comments": {
    "lineComment": "#"
  },
  "folding": {
    "markers": {
      "end": "^\\s*#\\s*endregion\\b",
      "start": "^\\s*#\\s*region\\b"
    }
  },
  "indentationRules": {
    "decreaseIndentPattern": "^\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$",
    "increaseIndentPattern": "^\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$"
  },
  "language": "yaml",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "wordPattern": "(-?\\b\\d+\\.\\d+\\b)|(-?\\b\\d+\\b)|([a-zA-Z_][a-zA-Z0-9_]*)|([\"'][^\"']*[\"'])"
}
