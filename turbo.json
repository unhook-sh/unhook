{
  "$schema": "https://turborepo.org/schema.json",
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event",
    "TURBO_TOKEN",
    "TURBO_TEAM",
    "NPM_TOKEN",
    "VERCEL_TOKEN"
  ],
  "globalEnv": ["NODE_ENV"],
  "tasks": {
    "//#clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "dist/**"
      ],
      "env": [
        "NEXT_PUBLIC_*",
        "CLERK_*",
        "SUPABASE_*",
        "POSTGRES_*",
        "POSTHOG_*",
        "OPENAI_API_KEY",
        "STRIPE_*",
        "RESEND_*"
      ]
    },
    "@unhook/web-app#build": {
      "env": [
        "NEXT_PUBLIC_*",
        "CLERK_*",
        "SUPABASE_*",
        "POSTGRES_*",
        "POSTHOG_*",
        "OPENAI_API_KEY",
        "STRIPE_*",
        "RESEND_*"
      ]
    },
    "@unhook/db#build": {
      "env": ["DATABASE_*", "POSTGRES_*", "SUPABASE_*"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "PORT",
        "NEXT_PUBLIC_*",
        "CLERK_*",
        "SUPABASE_*",
        "POSTGRES_*",
        "POSTHOG_*",
        "OPENAI_API_KEY",
        "STRIPE_*",
        "RESEND_*"
      ]
    },
    "format": {
      "dependsOn": ["^build"]
    },
    "gen-migration": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_*", "POSTGRES_*"]
    },
    "migrate": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_*", "POSTGRES_*"]
    },
    "push": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_*", "POSTGRES_*"]
    },
    "seed": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_*", "POSTGRES_*", "SUPABASE_*"]
    },
    "studio": {
      "cache": false,
      "persistent": true,
      "env": ["DATABASE_*", "POSTGRES_*"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "ui": "tui"
}
