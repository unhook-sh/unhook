{
  "commitlint": {
    "extends": [
      "@unhook/commitlint-config"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@commitlint/cli": "catalog:",
    "@testing-library/react": "catalog:",
    "@turbo/gen": "catalog:",
    "@types/bun": "catalog:",
    "@unhook/commitlint-config": "workspace:*",
    "bun": "catalog:",
    "checkly": "catalog:",
    "knip": "catalog:",
    "lefthook": "catalog:",
    "mintlify": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "name": "@unhook/root",
  "overrides": {
    "@types/vscode": "1.96.0"
  },
  "packageManager": "bun@1.2.18",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "bump-versions": "changeset version && bun install",
    "clean": "git clean -xdf node_modules",
    "clean:ws": "turbo run clean",
    "db:gen-migration": "turbo -F @unhook/db gen-migration",
    "db:migrate": "turbo -F @unhook/db migrate",
    "db:push": "turbo -F @unhook/db push",
    "db:seed": "turbo -F @unhook/db seed",
    "db:studio": "turbo -F @unhook/db studio",
    "dev": "turbo watch dev",
    "dev:next": "turbo watch dev -F @unhook/web-app...",
    "dev:vscode": "turbo watch dev -F unhook-vscode... -F @unhook/web-app...",
    "docs": "cd docs && mintlify dev",
    "format": "biome check",
    "format:ci": "biome ci",
    "format:fix": "biome check --write",
    "knip": "knip --include files,dependencies",
    "lint:ws": "bunx sherif@latest",
    "postinstall": "bun lint:ws",
    "publish": "turbo run build --filter=./apps/cli... --filter=./packages/client... --filter=./apps/vscode-extension... && changeset publish",
    "test": "bun test",
    "test:integ": "turbo run test --filter=@unhook/integ-test",
    "test:integ:ci": "turbo run test:ci --filter=@unhook/integ-test",
    "test:integ:watch": "turbo run test:watch --filter=@unhook/integ-test",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add",
    "version": "changeset version && bun biome check --write",
    "with-env": "infisical run --"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@clerk/shared",
