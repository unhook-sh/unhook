{
  "dependencies": {
    "@clerk/nextjs": "catalog:",
    "@modelcontextprotocol/sdk": "catalog:",
    "@unhook/analytics": "workspace:*",
    "@unhook/api": "workspace:*",
    "@unhook/db": "workspace:*",
    "@unhook/logger": "workspace:*",
    "@vercel/mcp-adapter": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@unhook/tsconfig": "workspace:*",
    "typescript": "catalog:"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "main": "./src/index.ts",
  "name": "@unhook/mcp-server",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist node_modules/.cache",
    "debug:server": "bun run src/debug-server.ts",
    "dev:inspector": "bun run src/dev-inspector.ts",
    "dev:server": "bun run src/dev-server.ts",
    "example": "tsx src/example.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },
  "types": "./src/index.ts",
  "version": "0.0.0"
}
